{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/Lan2Play/dedicated-server-api",
            "no-api": true
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "8.*",
        "laravel/framework": "^9.0",
        "laravelcollective/html": "^6.3.0",
        "laravel/legacy-factories": "^1.3",
        "webonyx/graphql-php": "dev-master",
        "invisnik/laravel-steam-auth": "^4.4.0",
        "laravel/socialite": "^5.5.3",
        "ignited/laravel-omnipay": "^3.4.0",
        "doctrine/dbal": "^3.3.7",
        "simplesoftwareio/simple-qrcode": "~4",
        "guzzlehttp/guzzle": "^7.4.5",
        "php-http/guzzle7-adapter": "^1.0.0",
        "team-reflex/challonge-php": "~4",
        "intervention/image": "^2.7.2",
        "facebook/graph-sdk": "^5.7",
        "laravel/tinker": "^2.7.2",
        "scssphp/scssphp": "^1.10.3",
        "omnipay/paypal": "^3.0.2",
        "cviebrock/eloquent-sluggable": "^9.0.0",
        "barryvdh/laravel-debugbar": "^3.7",
        "omnipay/stripe": "^3.2",
        "stripe/stripe-php": "^8.12",
        "symfony/routing": "^6.1",
        "sirprize/postal-code-validator": "^1.3.6",
        "artesaos/seotools": "^0.22.1",
        "weblagence/laravel-facebook-pixel": "^1.1",
        "spatie/laravel-cookie-consent": "^3.2.2",
        "xpaw/php-source-query-class": "^2.1.0",
        "maniaplanet/dedicated-server-api": "6.0.x-dev",
        "laravel/ui": "^4.0",
        "spatie/laravel-database-mail-templates": "^3.5",
        "laravel/sanctum": "^3.0",
        "propaganistas/laravel-phone": "^4.3.7",
        "symfony/mailgun-mailer": "^6.1",
        "symfony/http-client": "^6.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.5.0",
        "phpunit/phpunit": "^9.5.21",
        "kitloong/laravel-migrations-generator": "^6.3.2",
        "symfony/css-selector": "^6.1.0",
        "symfony/dom-crawler": "^6.1.0",
        "orangehill/iseed": "^3.0",
        "squizlabs/php_codesniffer": "^3.7.1",
        "filp/whoops": "^2.14.5"
    },
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": []
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
